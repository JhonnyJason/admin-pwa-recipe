{
    "staticBase": [
        ["merge"],
        {
            "merge": ["https://github.com/JhonnyJason/pwa-root-template"]
        }
    ],
    "output": [
        ["submodule"],
        ["merge"],
        {
            "merge": ["https://github.com/JhonnyJason/empty-base"]
        }
    ],
    "sources": [
        ["submodule"],
        ["create", "merge", "use"],
        {
            "create": ["admin-pwa-sources"],
            "merge": ["*"],
            "use": ["*"]
        }
    ],
    "pwa-sources": [
        ["submodule"],
        ["use"],
        {
            "use": ["*"]
        }
    ],
    "pwa-content": [
        ["directory"],
        ["use"],
        {
            "use": ["*"]
        }
    ],
    "testing": [
        ["directory"],
        ["merge"],
        {
            "merge": ["https://github.com/JhonnyJason/empty-base"]
        }
    ],
    "toolset": [
        ["submodule"],
        ["use"],
        {
            "use": ["https://github.com/JhonnyJason/toolset"]
        }
    ]
}
